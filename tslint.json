{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "member-access": [false],
    "ordered-imports": [false],
    "member-ordering": [false],
    "interface-name": [false],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "forin": false,
    "prefer-const": false
  },
  "rulesDirectory": [],
  "overrides": [
    {
      "files": ["**/*.ts?(x)"],
      "rules": {
        "react-hooks/exhaustive-deps": "off"
      }
    }
  ]
}
